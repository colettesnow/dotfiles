alias ls='eza -al --color=always --icons --group-directories-first --git'
alias la='eza -a --color=always --icons --group-directories-first --git'
alias ll='eza -l --color=always --icons --group-directories-first --git'
alias lt='eza -aT --color=always --icons --group-directories-first --git'
alias l.='eza -a | egrep "^\."'
alias ..='cd ..'
alias cd='z'
alias zmux='zellij'
alias fetchspecs="fastfetch -c paleofetch -l small"